<!-- Modal -->
<div class="row">
    <div class="col-md-6">
        <div class="pull-left">
            <button type="button" class="btn btn-default" data-toggle="modal" data-target="#modalBeneficiarios">Beneficiários</button>
        </div>
    </div>
</div>

<div class="modal fade" id="modalBeneficiarios" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <div class="row">
                    <div class="col-md-11">
                        <h5 class="modal-title" id="exampleModalLabel">Beneficiários</h5>
                    </div>
                    <div class="col-md-1">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="modal-body">
                <input type="hidden" id="identificadorBenfeficiario" />
                <div class="row">
                    <div class="col-md-5">
                        <div class="form-group">
                            <label for="CPF">CPF:</label>
                            <input type="text" class="form-control" id="beneficiarioCPF" name="beneficiarioCPF" placeholder="999.999.999-99" maxlength="14">
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="form-group">
                            <label for="Nome">Nome:</label>
                            <input type="text" class="form-control" id="beneficiarioNome" name="beneficiarioNome" placeholder="Ex.: João da Silva" maxlength="255">
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-group">
                            <label for="Sobrenome"></label>
                            <button type="button" class="btn btn-success" id="btnIncluirBeneficiario">Incluir</button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table id="tblBeneficiarios" class="table table-responsive table-bordered table-condensed">
                            <thead>
                                <tr>
                                    <td>CPF</td>
                                    <td>NOME</td>
                                    <td>Ações</td>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="limparCampos()">Fechar</button>
            </div>
        </div>
    </div>
</div>
<!-- Modal -->